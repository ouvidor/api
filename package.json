{
  "name": "ouvidor-api",
  "version": "1.0.0",
  "description": "API encarregada de prover endpoints e serviços às aplicações Mobile e Web do Ouvidor.",
  "main": "src/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "nodemon src/index.js",
    "start": "node build/bundle.js",
    "migrate": "yarn sequelize db:migrate",
    "migrate:undo:all": "yarn sequelize db:migrate:undo:all",
    "seed": "yarn sequelize db:seed:all",
    "pretest": "cross-env NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
    "posttest": "cross-env NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ouvidor/api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ouvidor/api/issues"
  },
  "homepage": "https://github.com/ouvidor/api#readme",
  "dependencies": {
    "@google-cloud/storage": "^4.7.0",
    "bcrypt": "^4.0.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "date-fns": "^2.13.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^3.1.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "node-geocoder": "^3.25.0",
    "nodemailer": "6.4.16",
    "nodemailer-express-handlebars": "^3.1.0",
    "sequelize": "^5.21.5",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/jest": "^25.1.3",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rollup": "^2.0.0",
    "rollup-plugin-analyzer": "^3.2.2",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-visualizer": "^3.3.1",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "sucrase": "^3.12.1",
    "supertest": "^4.0.2"
  }
}
