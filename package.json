    "url": "https://github.com/ouvidor/api/issues"
  },
  "homepage": "https://github.com/ouvidor/api#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.7.0",
    "sequelize": "^5.18.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/jest": "^24.0.18",
    "cross-env": "^6.0.0",
    "eslint": "^6.3.0",
